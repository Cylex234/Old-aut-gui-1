        return String:gsub("%s+", "") or String
      end

      local function FindPlayer(String)
        String = RemoveSpaces(String)
        for _, _Player in pairs(Players:GetPlayers()) do
          if _Player.Name:lower():match('^'.. String:lower()) then
            return _Player
          end
        end
        return nil
